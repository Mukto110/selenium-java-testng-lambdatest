<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="LambdaTest Automation Suite">
    <listeners>
        <listener class-name="com.lambdatest.framework.listeners.TestListener"/>
    </listeners>
    <test name="ChromeTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.lambdatest.tests.LoginTests"/>
        </classes>
    </test>
</suite>